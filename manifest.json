{
  "required_api_version": "^2.0.0",
  "name": "vlr.gg extension",
  "description": "Unofficial extension for the vlr.gg website",
  "developer_name": "Guido Neulaender",
  "icon": "images/icon.png",
  "options": {
    "query_debounce": 0.05
  },
  "preferences": [
    {
      "id": "vlr_gg",
      "type": "keyword",
      "name": "vlr.gg",
      "default_value": "vlr"
    },
    {
      "id": "item_name",
      "type": "input",
      "name": "Item name",
      "description": "Name for each item",
      "default_value": "Item"
    },
    {
      "id": "memo",
      "type": "text",
      "name": "Memo",
      "description": "Write something here",
      "default_value": ""
    }
  ]
}
